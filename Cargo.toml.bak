[package]
name = "sonicprobe"
version = "0.1.0"
edition = "2024"

[dependencies]
claxon = "0.4.3"
serde = {version = "1.0.219", features = ["derive"]}
serde_json = "1.0.142"

[dev-dependencies]
criterion = { version = "0.5", features = ["html_reports"] }
rand = "0.9.2"

[[bench]]
name = "dot_multiplication"
harness = false

[profile.release]
codegen-units = 1
lto = "thin"
